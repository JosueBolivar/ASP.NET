        <script>
        $(document).ready(function () {
            contador();
            $('#sidebarCollapse').on('click', function () {
                // open or close navbar
                $('#sidebar').toggleClass('active');
                // close dropdowns
                $('.collapse.in').toggleClass('in');
                // and also adjust aria-expanded attributes we use for the open/closed arrows
                // in our CSS
                $('a[aria-expanded=true]').attr('aria-expanded', 'false');

            });

            if ("@sessionName" != "" && "@sessionName" != null) {
                $("#menuvertical").load('@Url.Action("Menu", "Home", null, null)');
            }

            if (traevalor("_Login=")) {
                var valor = traevalor("_Login=");
                GuardarCookie("_Login", valor);
            }

        });

        var Tiempo = 900;
        //var Tiempo = 10;
        var totalTiempo = Tiempo;
        function contador() {
            if (traevalor("_Login=") != "" && traevalor("_Login=") != null) {

                var resta = function () {
                    totalTiempo -= 1;
                   var minutes = Math.floor(totalTiempo / 60);
                    var seconds = totalTiempo % 60;
                    minutes = minutes < 10 ? '0' + minutes : minutes;
                    seconds = seconds < 10 ? '0' + seconds : seconds;

                    document.getElementById('CuentaAtras').innerHTML = "Duración de la sesión: " + minutes + ":" + seconds + " minutos";

                    if (totalTiempo == 300) {
                    //if (totalTiempo == 5) {
                        var nombre = traevalor("_Login=");
                        window.myconfirm("Fin de sesión"," Le quedan 5 minutos, ¿Desea más tiempo? ").then(function () {
                            GuardarCookie("_Login", nombre)
                        }, function () {
                           // window.myalert("Importante !!!","La sesión se cerrará en 5 minutos aprox.");
                        });
                    }

                    if (totalTiempo == 0) {
                        BorrarCookie("_Login");
                        window.location.href='@Url.Action("Login", "Home")';
                    }
                }
                setInterval(resta, 1000);
            }
        }

        function traevalor(name) {
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return null;
        }
        function GuardarCookie(nombre, valor) {
            totalTiempo = Tiempo;
            document.cookie = nombre + "=" + valor + "; max-age=" + Tiempo + "; path=/;  domain=localhost"
        }

        function BorrarCookie(nombre) {
            //document.cookie = nombre + "=; expires=0 " + "; path=/;  domain=localhost"

        }

        function myalert(titulo, content) {
            $("<div></div>").kendoAlert({
                title: titulo,
                content: content
            }).data("kendoAlert").open();
        }

        function myconfirm(titulo, content) {
            return $("<div></div>").kendoConfirm({
                title: titulo,
                content: content,
                messages: {
                    okText: "SI",
                    cancel: "NO"
                }
            }).data("kendoConfirm").open().result;
        }
        </script>
